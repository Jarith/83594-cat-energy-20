<header class="header wrapper">
  <div class="header-container">
    {{#internalLink
      filename=filename
      url="index.html"
      className="logo header__logo"
    }}
      <img
        class="logo__title"
        src="https://via.placeholder.com/150x150"
        width="190"
        height="18"
        alt="Cat Energy"
      />
    {{/internalLink}}
    <button class="header__menu-button" type="button"></button>
  </div>
  <nav>
    <ul class="nav-menu nav-menu--open">
      <li class="nav-menu__item menu-item">
        {{#internalLink filename=filename url="index.html" className="menu-item__link"}}
          Главная
        {{/internalLink}}
      </li>
      <li class="nav-menu__item menu-item">
        {{#internalLink filename=filename url="catalog.html" className="menu-item__link"}}
          Каталог продукции
        {{/internalLink}}
      </li>
      <li class="nav-menu__item menu-item">
        {{#internalLink filename=filename url="form.html" className="menu-item__link"}}
          Подбор программы
        {{/internalLink}}
      </li>
    </ul>
  </nav>
</header>
